{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "outDir": "dist",
    "declaration": false,
    "module": "ESNext",
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  }
}
