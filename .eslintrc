{
  "extends": ["@antfu"],
  "rules": {
    "no-console": "warn",
    "no-tabs": "off",
    "indent": "off",
    "@typescript-eslint/indent": ["error", "tab"],
    "quotes": ["error", "double"],
    "@typescript-eslint/quotes": ["error", "double"],
    "semi": ["error", "always"],
    "@typescript-eslint/semi": ["error", "always"],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}
